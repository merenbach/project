.sw-tbl-head .sw-tbl-row .sw-tbl-cell {
    font-weight: bold;
    /*background-color: rgb(211, 219, 228);*/
    background-color: rgb(201, 209, 228);
    border-top: solid rgb(188, 198, 210) 1px;
}

/*border-bottom: solid rgb(188, 198, 210) 1px;*/

.sw-tbl-head .sw-tbl-row .sw-tbl-cell:first-child {
    -webkit-border-top-left-radius:	0.75em;
    -moz-border-radius-topleft:		0.75em;
    border-top-left-radius:			0.75em;
    border-left: solid rgb(188, 198, 210) 1px;
}

.sw-tbl-head .sw-tbl-row .sw-tbl-cell:last-child {
    -webkit-border-top-right-radius:	0.75em;
    -moz-border-radius-topright:		0.75em;
    border-top-right-radius:			0.75em;
    border-right: solid rgb(188, 198, 210) 1px;
}

/* detail */
ul.downloads {
    list-style-type: none;
    padding: 0;
    margin: 0;
}

ul.downloads li { margin-top: 0.375em; }
ul.downloads li:first-child { margin-top: 0; }
