 */

/*.sw-tbl-head .sw-tbl-row .sw-tbl-cell {
display: table-cell;
padding: 0.25em 0.5em;
}*/

.sw-tbl-head .sw-tbl-row {
    font-weight: bold;
}

/*.sw-tbl-row:last-child {
}*/

.sw-tbl-body .sw-tbl-row:last-child .sw-tbl-cell {
    border-bottom: solid rgb(188, 198, 210) 1px;
}

.sw-tbl-body .sw-tbl-row .sw-tbl-cell {
    border-top: solid rgb(188, 198, 210) 1px;
}

.sw-tbl-body .sw-tbl-row:last-child .sw-tbl-cell:first-child {
    -webkit-border-bottom-left-radius:	0.75em;
    -moz-border-radius-bottomleft:		0.75em;
    border-bottom-left-radius:			0.75em;
    border-left: solid rgb(188, 198, 210) 1px;
}

.sw-tbl-body .sw-tbl-row:last-child .sw-tbl-cell:last-child {
    -webkit-border-bottom-right-radius:	0.75em;
    -moz-border-radius-bottomright:		0.75em;
    border-bottom-right-radius:			0.75em;
    border-right: solid rgb(188, 198, 210) 1px;
}

