/* --- basic definitions --- */
.sw-tbl			{ display: table; }
.sw-tbl-head	{ display: table-header-group; }
.sw-tbl-body	{ display: table-row-group; }
.sw-tbl-foot	{ display: table-footer-group; }	/* unused */
.sw-tbl-row		{ display: table-row; }
.sw-tbl-cell	{ display: table-cell; }
/* --- end basic definitions --- */

.sw-tbl a, .sw-tbl a:visited {
    color: blue;	
}

.sw-tbl a:active {
    color: red;
}

.sw-tbl {
    width: 100%;
    margin: 0.75em 0;
}

/*.sw-tbl-head .sw-tbl-row {
border-bottom: solid rgb(188, 198, 210) 1px;
}
 */
/*.sw-tbl-body {
background-color: rgb(192, 255, 255);
border-left: solid rgb(188, 198, 210) 1px;
border-right: solid rgb(188, 198, 210) 1px;
}*/

.sw-tbl-row {
    padding: 0 0.5em;
}

/*.sw-tbl-body {
background-color: rgb(220, 255, 220);
}*/

.sw-tbl-row:nth-child(2n+1) .sw-tbl-cell {
    background-color: rgb(241, 249, 248);	
}

.sw-tbl-row:nth-child(2n) .sw-tbl-cell {
    background-color: rgb(231, 239, 238);	
}

.sw-tbl-row .sw-tbl-cell:first-child {
    border-left: solid rgb(188, 198, 210) 1px;
}

.sw-tbl-row .sw-tbl-cell:last-child {
    border-right: solid rgb(188, 198, 210) 1px;
}

.sw-tbl-row .sw-tbl-cell { padding: 0.25em 0.5em; }

